<t:layout xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd" xmlns:p="tapestry:parameter">
	<t:graphael.lineChart locx="50" locy="50" width="300" height="200">
		<t:graphael.lineSeries label="literal:Sin" values="sinValues" />
		<t:graphael.lineSeries label="literal:Cos" values="cosValues" />
		<t:graphael.lineSeries label="literal:Zero" values="zeroValues" />
		<p:postprocessor>linechartdemo.postprocessor</p:postprocessor>
	</t:graphael.lineChart>

	<h3>Source</h3>
	<t:stitch.tabGroup>
		<t:stitch.codeTab source="classpath:org/lazan/t5/graphael/demo/pages/LineChartDemo.tml" />
		<t:stitch.codeTab source="classpath:org/lazan/t5/graphael/demo/pages/LineChartDemo.java" />
	</t:stitch.tabGroup>
</t:layout> 